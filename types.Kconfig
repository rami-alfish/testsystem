config CONFIG_BOOL
    bool "Bool option"
    default y

config CONFIG_BOOL_DEP
    bool "Bool that depends on CONFIG_BOOL"
    depends on CONFIG_BOOL

config CONFIG_TRISTATE
    tristate "Tristate option"
    default m

config CONFIG_INT
    int "Integer option"
    range 0 100
    default 42

config CONFIG_HEX
    hex "Hexadecimal option"
    default 0x1F

config CONFIG_STRING
    string "String option"
    default "hello"



